{
  "manifest_version": 2,
 
  "name": "Wrike Templates CSS Modifier",
  "version": "0.1.0",
  "description": "Changes horizontal to vertical layout",
  "permissions": ["<all_urls>"],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }],
  "web_accessible_resources": ["styles.css"]
 
}

